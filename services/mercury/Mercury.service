[Unit]
Description=Manages all Mercury workers
After=network-online.target
Wants=network-online.target

[Service]
Type=oneshot
User=root
ExecStart=/usr/bin/sh /usr/local/tidepool/mercury/start_mercury_workers.sh
RemainAfterExit=yes
# Restart=always
# TimeoutStartSec=10
# RestartSec=10

[Install]
WantedBy = multi-user.target